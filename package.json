  "scripts": {
    "start": "SENTRY_RELEASE=$(sentry-cli releases propose-version) node scripts/start.js",
    "startWin": "node scripts/start",
    "startHttpsWin": "set HTTPS=true&&set PORT=443&&npm run startWin",
    "startHttpsMac": "HTTPS=true PORT=443 npm start",
    "start80": "PORT=80 npm start",
    "build": "node scripts/build.js",
    "test": "NODE_ICU_DATA=node_modules/full-icu jest --runInBand",
    "testWithLog": "NODE_ICU_DATA=node_modules/full-icu jest --json --outputFile=JestFails.json",
    "testLocal": "node scripts/test.js",
    "storybook": "IS_STORYBOOK=1 start-storybook -p 9009 -s public",
    "build-storybook": "IS_STORYBOOK=1 build-storybook -s public",
    "cypress-test": "npx cypress run --project ./di-cypress",
    "cypress-local": "CYPRESS_BASE_URL=https://localhost:44300 npx cypress run --project ./di-cypress"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie < 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
