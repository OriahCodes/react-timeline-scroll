{
    "name": "react-timeline-scroll",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/OriahCodes/react-timeline-scroll.git"
    },
    "peerDependencies": {
        "react": "^16.0.2",
        "react-dom": "^16.0.2"
    },
    "scripts": {
        "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.0",
        "@babel/core": "^7.17.2",
        "@babel/preset-react": "^7.16.7",
        "babel-plugin-macros": "^3.1.0",
        "@babel/plugin-transform-modules-commonjs": "^7.16.8"
    },
    "dependencies": {
        "@babel/polyfill": "^7.12.1"
    },
    "babel": {
        "presets": [
            "react"
        ],
        "plugins": [
            "macros",
            [
                "@babel/plugin-transform-modules-commonjs",
                {
                    "allowTopLevelThis": true
                }
            ]
        ]
    }
}