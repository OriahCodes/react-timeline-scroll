  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "jest-canvas-mock",
      "./src/Jest/MockImage.js",
      "./src/Jest/stubWinLoc.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironmentOptions": {
      "resources": "usable"
    },
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.aaa$": "jest-raw-loader",
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "@redux/store": "<rootDir>/src/redux/store.js",
      "@redux/(.*)$": "<rootDir>/src/redux/$1",
      "@dashboardView-redux/actions/AssetList": "<rootDir>/mocks/dashboard/AssetList.js",
      "@dashboardView-redux/reducers/OnAssetListReady": "<rootDir>/src/Views/DashboardView/redux/reducers/OnAssetListReady.js",
      "@dashboardView-redux/reducers/OnInfSelected": "<rootDir>/src/Views/DashboardView/redux/reducers/OnInfSelected.js",
      "@dashboardView-redux/actions/Inf": "<rootDir>/mocks/dashboard/AssetList.js",
      "@dashboardView-redux/actions/(.*)$": "<rootDir>/src/Views/DashboardView/redux/actions/$1",
      "@assetView-redux/actions/(.*)$": "<rootDir>/src/Views/AssetView/redux/actions/$1",
      "@assetView-redux/reducers/(.*)$": "<rootDir>/src/Views/AssetView/redux/reducers/$1",
      "@assetView-redux/store": "<rootDir>/src/Views/AssetView/redux/store.js",
      "@analyticsView-redux/(.*)$": "<rootDir>/src/Views/AnalyticsView/redux/$1",
      "@mocks(.*)$": "<rootDir>/mocks$1",
      "@dashboardView-redux/store": "<rootDir>/src/Views/DashboardView/redux/store.js",
      "@/APIs/Assets": "<rootDir>/mocks/APIs/Assets.js",
      "@/APIs/Users": "<rootDir>/mocks/APIs/Users.js",
      "@/APIs/User": "<rootDir>/mocks/APIs/User.js",
      "@/APIs/Shares": "<rootDir>/mocks/APIs/Shares.js",
      "@/APIs/minShare": "<rootDir>/mocks/APIs/minShare.js",
      "@/APIs/FindingsHistory": "<rootDir>/mocks/APIs/FindingsHistory.js",
      "@/APIs/AlertNotifications": "<rootDir>/mocks/APIs/AlertNotifications.js",
      "@/APIs/surveyHaifa.json": "<rootDir>/src/APIs/surveyHaifa.json",
      "@/APIs/Notifications": "<rootDir>/mocks/APIs/Notifications.js",
      "@/APIs/PresignResourcesUrl": "<rootDir>/mocks/APIs/PresignResourcesUrl.js",
      "@/APIs/Resources": "<rootDir>/mocks/APIs/Resources.js",
      "@/APIs/InfFiles": "<rootDir>/mocks/APIs/InfFiles.js",
      "@/APIs/Findings": "<rootDir>/mocks/APIs/Findings.js",
      "@/APIs/AddFinding": "<rootDir>/mocks/APIs/AddFinding.js",
      "@/APIs/DeleteFile": "<rootDir>/mocks/APIs/DeleteFile.js",
      "@/APIs/ShareWhat": "<rootDir>/mocks/APIs/ShareWhat.js",
      "@/APIs/DeleteFinding": "<rootDir>/mocks/APIs/DeleteFinding.js",
      "@/APIs/FindingFiles": "<rootDir>/mocks/APIs/FindingFiles.js",
      "@/APIs/Events": "<rootDir>/mocks/APIs/Events.js",
      "@sentry/react": "<rootDir>/mocks/Sentry.js",
      "@globals/Config": "<rootDir>/mocks/Config.js",
      "@/APIs/SortTypes": "<rootDir>/mocks/APIs/SortTypes.js",
      "@globals(.*)$": "<rootDir>/src/globals$1",
      "@components(.*)$": "<rootDir>/src/components$1",
      "@images(.*)$": "<rootDir>/src/images$1",
      "amazon-cognito-identity-js": "<rootDir>/mocks/Cognito.js",
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "macros",
      [
        "@babel/plugin-transform-modules-commonjs",
        {
          "allowTopLevelThis": true
